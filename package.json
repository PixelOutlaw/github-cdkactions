{
  "name": "@mythicdrops/github-cdkactions",
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/MythicDrops/github-cdkactions.git"
  },
  "author": "Richard Harrah",
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "npm": "^7"
  },
  "main": "dist/index.js",
  "module": "dist/github-cdkactions.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "commit": "cz",
    "lint": "tsdx lint",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "prepare": "tsdx build",
    "prepublishOnly": "pinst --disable",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cdkactions": "0.2.3",
    "constructs": "3.3.75",
    "ts-dedent": "2.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@size-limit/file": "4.11.0",
    "@types/eslint": "7.2.11",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/is-ci": "3.0.0",
    "@types/js-yaml": "4.0.1",
    "@types/node": "15.6.0",
    "@types/prettier": "2.2.3",
    "@types/semantic-release": "17.2.0",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "@yarnpkg/pnpify": "2.4.0",
    "babel-eslint": "10.1.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "is-ci": "3.0.0",
    "js-yaml": "4.1.0",
    "pinst": "2.1.6",
    "prettier": "2.3.0",
    "prettier-plugin-organize-imports": "2.1.0",
    "prettier-plugin-pkg": "0.9.0",
    "prettier-plugin-sh": "0.6.1",
    "pretty-quick": "3.1.0",
    "semantic-release": "17.4.3",
    "size-limit": "4.11.0",
    "tsdx": "0.14.1",
    "tslib": "2.2.0",
    "typescript": "4.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "settings": {
      "react": {
        "version": "999.999.999"
      }
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "size-limit": [
    {
      "path": "dist/github-cdkactions.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/github-cdkactions.esm.js",
      "limit": "10 KB"
    }
  ],
  "volta": {
    "node": "14.17.0",
    "npm": "7.14.0"
  }
}
